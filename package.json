{
  "name": "hazy",
  "version": "0.1.16",
  "description": "Lazy JSON fixtures in Node",
  "main": "./lib/hazy.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "eslint src/hazy.js",
    "start": "babe-node src/hazy.js",
    "pretest": "npm run build",
    "test": "mocha --reporter nyan --compilers js:babel-core/register test",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore public/js",
    "prepublish": "npm run build",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/slurmulon/hazy.git"
  },
  "keywords": [
    "hazy",
    "stub",
    "json",
    "fixtures",
    "random",
    "query",
    "jsonpath",
    "test",
    "dry",
    "lazy"
  ],
  "author": "slurmulon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slurmulon/hazy/issues"
  },
  "dependencies": {
    "chance": "~1.0.3",
    "glob": "^7.0.3",
    "jsonpath": "^0.2.4",
    "lodash.bindkey": "^4.1.4",
    "lodash.drop": "^4.0.4",
    "lodash.foreach": "^4.3.0",
    "lodash.get": "^4.3.0",
    "lodash.isempty": "^4.2.1",
    "lodash.isplainobject": "^4.0.4",
    "lodash.map": "^4.4.0",
    "lodash.mapvalues": "^4.4.0",
    "lodash.memoize": "^4.1.0",
    "lodash.reduce": "^4.4.0",
    "lodash.reject": "^4.4.0",
    "lodash.template": "^4.2.5",
    "lodash.values": "^4.1.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-things": "^0.2.0",
    "eslint": "^2.12.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3"
  }
}
