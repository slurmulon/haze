{
  "name": "hazy",
  "version": "0.1.14",
  "description": "Lazy JSON fixtures in Node",
  "main": "./lib/hazy.js",
  "scripts": {
    "build": "node ./node_modules/.bin/babel src --out-dir lib",
    "lint": "eslint src/hazy.js",
    "start": "./node_modules/.bin/babel-node src/hazy.js",
    "pretest": "npm run build",
    "test": "node ./node_modules/.bin/mocha --reporter nyan --compilers js:babel-core/register test",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore public/js",
    "preinstall": "npm install --ignore-scripts && npm run build",
    "prepublish": "npm run build",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/slurmulon/hazy.git"
  },
  "keywords": [
    "hazy",
    "stub",
    "json",
    "fixtures",
    "random",
    "query",
    "jsonpath",
    "test",
    "dry",
    "lazy"
  ],
  "author": "MadHax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slurmulon/hazy/issues"
  },
  "dependencies": {
    "chance": "~0.7.5",
    "glob": "^5.0.10",
    "jsonpath": "^0.1.4",
    "lodash.bindkey": "^4.1.2",
    "lodash.drop": "^4.0.1",
    "lodash.foreach": "^4.1.0",
    "lodash.get": "^4.1.2",
    "lodash.isempty": "^4.1.3",
    "lodash.isplainobject": "^4.0.3",
    "lodash.map": "^4.2.1",
    "lodash.mapvalues": "^4.2.0",
    "lodash.memoize": "^4.0.1",
    "lodash.reduce": "^4.2.0",
    "lodash.reject": "^4.2.1",
    "lodash.template": "^4.2.2",
    "lodash.values": "^4.1.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "blanket": "^1.2.1",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "eslint": "^1.1.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.2",
    "nodemon": "^1.4.1"
  }
}
