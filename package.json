{
  "name": "hazy",
  "version": "0.1.1",
  "description": "Lazy JSON fixtures",
  "main": "./lib/hazy.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "eslint src/hazy.js",
    "start": "babel-node src/hazy.js",
    "pretest": "npm run build",
    "test": "mocha --reporter nyan --compilers js:babel/register test",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore public/js",
    "postinstall": "npm run build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/slurmulon/hazy.git"
  },
  "keywords": [
    "hazy",
    "stub",
    "json",
    "fixtures",
    "lazy"
  ],
  "author": "MadHax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slurmulon/hazy/issues"
  },
  "dependencies": {
    "acho": "^2.3.0",
    "babel": "^5.8.21",
    "chance": "~0.7.5",
    "glob": "^5.0.10",
    "jsonpath": "^0.1.4",
    "lodash": "~3.9.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.10",
    "blanket": "~1.1.7",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "eslint": "^1.1.0",
    "mocha": "^2.3.2",
    "nodemon": "^1.4.1"
  }
}
